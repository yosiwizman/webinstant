    "dotenv": "^17.2.1",
    "lucide-react": "^0.540.0",
    "next": "^15.5.0",
    "next-themes": "^0.2.1",
    "openai": "^5.12.2",
    "papaparse": "^5.5.3",
    "puppeteer": "^24.16.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "recharts": "^2.12.7",
    "replicate": "^1.0.1",
    "resend": "^6.0.1",
    "serpapi": "^2.2.1",
    "stripe": "^18.4.0",
